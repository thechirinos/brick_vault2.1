<%= simple_form_for(@lego_set) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :set %>
    <%= f.input :name %>
    <%= f.input :link %>
    <%= f.association :user %>
    <%= f.input :description %>
    <%= f.input :theme %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
